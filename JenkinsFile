pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
                bat "rmdir  /s /q PostEx."
                bat "git clone https://github.com/RimshaIsmailSiddiqui/PostEx..git"
                bat "mvn clean -f PostEx."
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f PostEx."
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f PostEx."
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f PostEx."
            }
        }
    }
}
